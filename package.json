    "axios": "^0.18.1",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "d3": "^5.9.2",
    "express": "^4.16.4",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^8.1.0",
