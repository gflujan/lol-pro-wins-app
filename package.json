  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "d3": "^7.6.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
